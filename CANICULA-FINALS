                            Workout workout20 = new Workout("Bench Press ", 3, 10);
                            weeklyWorkouts["FRIDAY"].Add(workout20);
                            Workout workout21 = new Workout("Inclined Dumbell Press", 3, 10);
                            weeklyWorkouts["FRIDAY"].Add(workout21);
                            Workout workout22 = new Workout("Chest Flyes", 3, 15);
                            weeklyWorkouts["FRIDAY"].Add(workout22);
