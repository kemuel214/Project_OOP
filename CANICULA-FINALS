                            weeklyWorkouts["FRIDAY"].Add(workout23);
                            Workout workout24 = new Workout("Dips", 3, 15);
                            weeklyWorkouts["FRIDAY"].Add(workout24);

                            Workout workout25 = new Workout("Deadlift", 3, 10);
                            weeklyWorkouts["SATURDAY"].Add(workout25);
                            Workout workout26 = new Workout("Lat Pulldowns", 3, 10);
                            weeklyWorkouts["SATURDAY"].Add(workout26);
                            Workout workout27 = new Workout("Barbell Rows", 3, 12);
                            weeklyWorkouts["SATURDAY"].Add(workout27);
                            Workout workout28 = new Workout("Preacher Curls", 3, 12);
                            weeklyWorkouts["SATURDAY"].Add(workout28);
                            Workout workout29 = new Workout("Hammer Curls", 3, 12);
                            weeklyWorkouts["SATURDAY"].Add(workout29);
                        }
                    }
                    break;
                case "5":
                    bmr *= 1.9;
                    Console.Write("Do you want a recommended workout plan?[Y/N]: ");
                    a = char.Parse(Console.ReadLine());
                    Console.WriteLine();
                    if (char.ToUpper(a) == 'Y')
                    {
                        if (age >= 14 && age <= 17)
                        {
                            Workout workout1 = new Workout("Bench Press ", 4, 10);
                            weeklyWorkouts["MONDAY"].Add(workout1);
                            Workout workout2 = new Workout("Inclined Dumbell Press", 4, 10);
                            weeklyWorkouts["MONDAY"].Add(workout2);
