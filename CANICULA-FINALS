                                Workout workout22 = new Workout("Chest Flyes", 4, 15);
                                weeklyWorkouts["FRIDAY"].Add(workout22);
                                Workout workout23 = new Workout("Tricep Push Down", 4, 12);
                                weeklyWorkouts["FRIDAY"].Add(workout23);
                                Workout workout24 = new Workout("Dips", 4, 15);
                                weeklyWorkouts["FRIDAY"].Add(workout24);

                                Workout workout25 = new Workout("Deadlift", 4, 10);
                                weeklyWorkouts["SATURDAY"].Add(workout25);
                                Workout workout26 = new Workout("Lat Pulldowns", 4, 10);
                                weeklyWorkouts["SATURDAY"].Add(workout26);
                                Workout workout27 = new Workout("Barbell Rows", 4, 12);
                                weeklyWorkouts["SATURDAY"].Add(workout27);
                                Workout workout28 = new Workout("Preacher Curls", 4, 12);
                                weeklyWorkouts["SATURDAY"].Add(workout28);
                                Workout workout29 = new Workout("Hammer Curls", 4, 12);
                                weeklyWorkouts["SATURDAY"].Add(workout29);
                            }
                        }
                        break;
                    default:
                        Console.Clear();
                        Console.WriteLine("Invalid option");
                        continue;
            }
            Console.Clear();
            Console.WriteLine($"Your BMR is {bmr:F0}");
            Console.WriteLine("Your Recommended Workout plan:");
            foreach (var kvp in weeklyWorkouts)
            {
                Console.WriteLine($"{kvp.Key} Workouts:");

                if (kvp.Value.Count == 0)
                {
                    Console.WriteLine("REST DAY");
                }
                else
                {
                    for (int i = 0; i < kvp.Value.Count; i++)
                    {
                        Console.WriteLine($"[{i + 1}] {kvp.Value[i].Name} - {kvp.Value[i].Sets} sets - {kvp.Value[i].Reps} reps");
                    }
                }
                Console.WriteLine();
            }
            break;
        }
        Console.WriteLine("PRESS ANY KEY TO CONTINUE...");
        Console.ReadKey();
    }
}

class Female : BMR
{
    private Dictionary<string, List<Workout>> weeklyWorkouts; // Add this field

    public Female(int age, double weight, double height, Dictionary<string, List<Workout>> weeklyWorkouts)
        : base(age, weight, height)
    {
        this.weeklyWorkouts = weeklyWorkouts;
    }

    public override void CalculateBMR()
    {
        double bmr = (10 * weight) + (6.25 * height) - (5 * age) - 161;
        while (true)
        {
            Console.WriteLine("Enter intensity");
            Console.WriteLine("1: Sedentary (little or no exercise)");
            Console.WriteLine("2: Lightly active (exercise 1–3 days/week)");
            Console.WriteLine("3: Moderately active (exercise 3–5 days/week)");
            Console.WriteLine("4: Active (exercise 6–7 days/week)");
            Console.WriteLine("5: Very active (hard exercise 6–7 days/week)");
            Console.WriteLine("_________________________________________________");


            string c = Console.ReadLine();
            switch (c)
            {
                case "1":
                    bmr *= 1.2;
                    Console.Write("Do you want a recommended workout plan?[Y/N]: ");
                    char a = char.Parse(Console.ReadLine());
                    Console.WriteLine();
                    if (char.ToUpper(a) == 'Y')
                    {
                        if (age >= 14 && age <= 17)
                        {
                            Workout workout1 = new Workout("Squats", 3, 12);
                            weeklyWorkouts["MONDAY"].Add(workout1);
                            Workout workout2 = new Workout("Lunges", 3, 12);
                            weeklyWorkouts["MONDAY"].Add(workout2);
                            Workout workout3 = new Workout("Glute Bridges", 3, 12);
                            weeklyWorkouts["MONDAY"].Add(workout3);
                            Workout workout4 = new Workout("Crunches", 3, 10);
                            weeklyWorkouts["WEDNESDAY"].Add(workout4);
                            Workout workout5 = new Workout("Leg raises", 3, 8);
                            weeklyWorkouts["WEDNESDAY"].Add(workout5);
                            Workout workout6 = new Workout("Russian Twist", 3, 12);
                            weeklyWorkouts["WEDNESDAY"].Add(workout6);
                            Workout workout7 = new Workout("Burpees", 3, 10);
                            weeklyWorkouts["FRIDAY"].Add(workout7);
                            Workout workout8 = new Workout("Inclined Push Ups", 3, 10);
                            weeklyWorkouts["FRIDAY"].Add(workout8);
                            Workout workout9 = new Workout("Jumping Jacks", 3, 10);
                            weeklyWorkouts["FRIDAY"].Add(workout9);
                        }
                        else if (age >= 18 && age <= 30)
                        {
                            Workout workout1 = new Workout("Squats", 3, 12);
                            weeklyWorkouts["MONDAY"].Add(workout1);
                            Workout workout2 = new Workout("Lunges", 3, 12);
                            weeklyWorkouts["MONDAY"].Add(workout2);
